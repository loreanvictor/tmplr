{
  "name": "tmplr",
  "version": "0.3.3",
  "description": "Easy templating for repositories",
  "bin": "dist/src/index.js",
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "build:clean": "npm run clean && npm run build",
    "start": "ts-node ./src/index.tsx",
    "test": "npm run build:clean && chmod +x dist/src/index.js && cross-env NODE_OPTIONS=--experimental-vm-modules jest",
    "coverage": "jest --coverage",
    "prepack": "npm run build:clean"
  },
  "files": [
    "dist/"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/loreanvictor/tmplr.git"
  },
  "keywords": [
    "template",
    "cli",
    "repository",
    "setup"
  ],
  "author": "Eugene Ghanizadeh Khoub",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/loreanvictor/tmplr/issues"
  },
  "homepage": "https://github.com/loreanvictor/tmplr#readme",
  "dependencies": {
    "@tmplr/core": "^0.8.1",
    "@tmplr/node": "^0.2.3",
    "@tmplr/react": "^0.2.0",
    "@tmplr/yaml-parser": "^0.2.0",
    "chalk": "^4.1.2",
    "cli-highlight": "^2.1.11",
    "date-and-time": "^3.0.2",
    "ink": "^3.2.0",
    "ink-select-input": "^4.2.1",
    "ink-spinner": "^4.0.3",
    "ink-text-input": "^4.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-use": "^17.4.0",
    "semver": "^7.3.7",
    "terminal-link": "^2.1.1",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@sindresorhus/tsconfig": "^2.0.0",
    "@types/degit": "^2.8.3",
    "@types/git-url-parse": "^9.0.1",
    "@types/jest": "^27.5.2",
    "@types/node": "^17.0.31",
    "@types/react": "^18.0.9",
    "@types/semver": "^7.3.9",
    "@types/yargs": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "babel-jest": "^28.1.0",
    "ci-info": "^4.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.15.0",
    "eslint-plugin-jest-extended": "^2.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "execa": "^7.2.0",
    "get-bin-path": "^10.0.0",
    "ink-testing-library": "^2.1.0",
    "jest": "^28.1.0",
    "jest-extended": "^4.0.2",
    "sleep-promise": "^9.1.0",
    "ts-jest": "^28.0.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  }
}
