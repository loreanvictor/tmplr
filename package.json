{
  "name": "tmplr",
  "version": "0.0.1",
  "description": "Easy templating for repositories",
  "bin": "dist/cli.js",
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "build": "tsc",
    "start": "npm run build && chmod +x dist/cli.js && dist/cli.js",
    "pretest": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepack": "npm run build"
  },
  "files": [
    "dist/"
  ],
  "repository": {
