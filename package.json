  "dependencies": {
    "chalk": "^5.0.1",
    "degit": "^2.8.4",
    "git-url-parse": "^11.6.0",
    "ink": "^3.2.0",
    "ink-spinner": "^4.0.3",
    "ink-text-input": "^4.0.3",
    "react": "^17.0.2",
    "react-use": "^17.3.2",
    "simple-git": "^3.7.1",
    "yaml": "^2.0.1"
  },
  "devDependencies": {
    "@sindresorhus/tsconfig": "^2.0.0",
    "@types/chalk": "^2.2.0",
